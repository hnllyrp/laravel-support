{
    "name": "hnllyrp/laravel-support",
    "description": "The laravel support library",
    "keywords": [
        "php",
        "library",
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hnllyrp",
            "email": "hnllyrp@126.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "ext-openssl": "*",
        "ext-gmp": "*",
        "firebase/php-jwt": "^5.2",
        "laravel/framework": "^6.0",
        "lpilp/guomi": "^1.0.8"
    },
    "autoload": {
        "psr-4": {
            "Hnllyrp\\LaravelSupport\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Hnllyrp\\LaravelSupport\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hnllyrp\\LaravelSupport\\SupportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
